{
  "name": "animora-ui",
  "version": "0.0.1",
  "description": "Animation-driven Vue component library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./animations": {
      "import": "./dist/animations/index.es.js",
      "require": "./dist/animations/index.cjs.js",
      "types": "./dist/animations/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly"
  },
  "peerDependencies": {
    "vue": "^2.7.0 || ^3.0.0",
    "gsap": "^3.12.0"
  },
  "peerDependenciesMeta": {
    "gsap": {
      "optional": true
    }
  },
  "dependencies": {
    "vue-demi": "^0.14.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "sass": "^1.69.0",
    "vite": "^5.0.0",
    "vue": "^3.3.0",
    "vue-tsc": "^1.8.0"
  },
  "keywords": [
    "vue",
    "animation",
    "ui",
    "component"
  ],
  "author": "",
  "license": "MIT"
}
